{
  "name": "agent-react-devtools",
  "version": "0.2.0",
  "description": "CLI tool for AI agents to inspect React component trees and profile performance",
  "type": "module",
  "bin": {
    "agent-react-devtools": "./dist/cli.js"
  },
  "exports": {
    ".": "./dist/cli.js",
    "./connect": {
      "types": "./dist/connect.d.ts",
      "import": "./dist/connect.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "prepublishOnly": "cp ../../README.md ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/piotrski/agent-react-devtools.git",
    "directory": "packages/agent-react-devtools"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.0",
    "react-devtools-core": "^6.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vite": "^6.0.0",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "ws": "^8.18.0",
    "zod": "^3.24.0"
  },
  "peerDependencies": {
    "react-devtools-core": ">=5.0.0",
    "vite": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "react-devtools-core": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  }
}
